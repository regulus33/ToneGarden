var CACHE_VERSION = 'v1';
var CACHE_NAME = CACHE_VERSION + ':sw-cache-APPNAME';
// TODO: in depth cacheing
// Create a local server with a symmetrical relation to actual server
class BinauralBeatsController {
  static show(){}
}

//Install callback this inits all
function onInstall(event) {
    event.waitUntil(
        caches.open(CACHE_NAME).then(function prefill(cache) {
            return cache.addAll([
                '<%= asset_pack_path 'application.js' %>',
                '<%= asset_pack_path 'application.css' %>',
               '/offline.html'
            ]);
        })
    );
}

function onFetch(event){
  const url = event.request.url
}

self.addEventListener('install', onInstall)

self.addEventListener('fetch', onFetch)
